    "@storybook/react": "^6.1.11",
    "@types/jest": "^25.1.4",
    "@types/node": "^14.14.20",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@types/react-pdf": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
